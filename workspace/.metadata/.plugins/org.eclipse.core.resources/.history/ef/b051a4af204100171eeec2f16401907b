/**
 * 
 */
package com.parvanpajooh.client.geomanagement.model.criteria;

import com.parvanpajooh.client.common.BaseMsg;
import com.parvanpajooh.client.rateandroute.model.enums.RateAndRouteEnums.ProductStatus;

/**
 * @author Mohammad
 *
 */
public class ProductCriteriaMsg extends BaseMsg {

	/**
	 * 
	 */
	private static final long serialVersionUID = -2255038419857740535L;
	
	private Long id;
	
	private String name;
	
	private String code;
	
	private Boolean customerBase;
	
	private Boolean active;
	
	private ProductStatus status;

	/**
	 * @return the id
	 */
	public Long getId() {
		return id;
	}

	/**
	 * @param id the id to set
	 */
	public void setId(Long id) {
		this.id = id;
	}

	/**
	 * @return the name
	 */
	public String getName() {
		return name;
	}

	/**
	 * @param name the name to set
	 */
	public void setName(String name) {
		this.name = name;
	}

	/**
	 * @return the code
	 */
	public String getCode() {
		return code;
	}

	/**
	 * @param code the code to set
	 */
	public void setCode(String code) {
		this.code = code;
	}

	/**
	 * @return the customerBase
	 */
	public Boolean getCustomerBase() {
		return customerBase;
	}

	/**
	 * @param customerBase the customerBase to set
	 */
	public void setCustomerBase(Boolean customerBase) {
		this.customerBase = customerBase;
	}

	/**
	 * @return the active
	 */
	public Boolean getActive() {
		return active;
	}

	/**
	 * @param active the active to set
	 */
	public void setActive(Boolean active) {
		this.active = active;
	}

	/**
	 * @return the status
	 */
	public ProductStatus getStatus() {
		return status;
	}

	/**
	 * @param status the status to set
	 */
	public void setStatus(ProductStatus status) {
		this.status = status;
	}

	/* (non-Javadoc)
	 * @see java.lang.Object#toString()
	 */
	@Override
	public String toString() {
		return "ProductCriteria [id=" + id + ", name=" + name + ", code="
				+ code + ", customerBase=" + customerBase + ", active="
				+ active + ", status=" + status + "]";
	}
	
}
