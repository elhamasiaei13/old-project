package com.parvanpajooh.sample.model.criteria;

import com.parvanpajooh.common.vo.BaseCriteria;

public class SampleCriteria extends BaseCriteria {
	
	/**
	 * 
	 */
	private static final long serialVersionUID = -6692696276170013041L;

	private String title;
	
	private String USER_NAME;
	private String PASSWORD;
	private String FIRST_NAME;
	private String LAST_NAME;

	

	
	/* (non-Javadoc)
	 * @see java.lang.Object#toString()
	 */
	@Override
	public String toString() {
		return "SampleCriteria [title=" + title + ", firstName=" + USER_NAME + ", password="
				+ PASSWORD + ", firstName=" + FIRST_NAME + ", lastName=" + LAST_NAME + "]";
	}

	/**
	 * @return the title
	 */
	public String getTitle() {
		return title;
	}

	/**
	 * @param title the title to set
	 */
	public void setTitle(String title) {
		this.title = title;
	}

	

}
