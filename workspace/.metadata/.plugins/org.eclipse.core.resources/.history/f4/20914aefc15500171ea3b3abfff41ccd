package com.parvanpajooh.client.transportationmanagement2.model;

import com.parvanpajooh.client.common.BaseMsg;
import com.parvanpajooh.client.transportationmanagement2.model.TransportationManagementEnums.TransportationScheduleStatus;

/**
 * @author sasan
 */

public class TransportationScheduleMsg extends BaseMsg{

	private static final long serialVersionUID = -1304277576944253813L;
	
	private Long id;
	
	private boolean scheduled;

	private String vehicleCarrierRegister;

	private String carrierCode;
	
	private String routeNumber;

	private String fromNodeCode;

	private String toNodeCode;

	private Integer departureDateInUtc;
	
	private Integer departureDateInOriginLocal;
	
	private String departureTimeInUtc;
	
	private String departureTimeInOriginLocal;
	
	private Integer arrivalDateInUtc;
	
	private Integer arrivalDateInDestinationLocal;
	
	private String arrivalTimeInUtc;
	
	private String arrivalTimeInDestinationLocal;
	
	private Double totalWeight;
	
	private Double reservableWeight;
	
	private TransportationScheduleStatus status;
	

	public Long getId() {
		return id;
	}

	public void setId(Long id) {
		this.id = id;
	}

	public String getCarrierCode() {
		return carrierCode;
	}

	public void setCarrierCode(String carrierCode) {
		this.carrierCode = carrierCode;
	}

	public String getRouteNumber() {
		return routeNumber;
	}

	public String getVehicleCarrierRegister() {
		return vehicleCarrierRegister;
	}

	public void setVehicleCarrierRegister(String vehicleCarrierRegister) {
		this.vehicleCarrierRegister = vehicleCarrierRegister;
	}

	public void setRouteNumber(String routeNumber) {
		this.routeNumber = routeNumber;
	}

	public String getFromNodeCode() {
		return fromNodeCode;
	}

	public void setFromNodeCode(String fromNodeCode) {
		this.fromNodeCode = fromNodeCode;
	}

	public String getToNodeCode() {
		return toNodeCode;
	}

	public void setToNodeCode(String toNodeCode) {
		this.toNodeCode = toNodeCode;
	}
	
	
	
}
