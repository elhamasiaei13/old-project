package com.parvanpajooh.agentmanagement.common;

import java.util.Map;


import com.parvanpajooh.accounting.AccountingProtos;

import retrofit2.Call;
import retrofit2.http.GET;
import retrofit2.http.Path;
import retrofit2.http.Query;

public interface AccountingService {
	

	
	@GET("v1/accounts")
	public Call<AccountingProtos.AccountList> findAccounts(@Query("userName") String userName, @Query("ip") String ip);
	

}
