package com.parvanpajooh.client.geomanagement.model.criteria;

import com.parvanpajooh.client.common.BaseMsg;
import com.parvanpajooh.client.rateandroute.model.NodeMsg;
import com.parvanpajooh.client.rateandroute.model.ProductMsg;
import com.parvanpajooh.client.rateandroute.model.RateTypeMsg;
import com.parvanpajooh.client.rateandroute.model.RouteMsg;
import com.parvanpajooh.client.rateandroute.model.enums.RateAndRouteEnums.DiscountStatus;

public class DiscountCriteriaMsg extends BaseMsg {
	
	/**
	 * 
	 */
	private static final long serialVersionUID = 6157076733166141708L;

	private String code;
	
	private String action;
	
	private DiscountStatus status;

	private String waybillNumber;
	
	private Double weight;
	
	private String agentCode;
	
	private ProductMsg product;
	
	private RateTypeMsg rateType;
	
	private RouteMsg route;
	
	private NodeMsg fromNode;
	
	private NodeMsg toNode;
	
	/**
	 * @return the code
	 */
	public String getCode() {
		return code;
	}

	/**
	 * @param code the code to set
	 */
	public void setCode(String code) {
		this.code = code;
	}

	/**
	 * @return the action
	 */
	public String getAction() {
		return action;
	}

	/**
	 * @param action the action to set
	 */
	public void setAction(String action) {
		this.action = action;
	}

	/**
	 * @return the status
	 */
	public DiscountStatus getStatus() {
		return status;
	}

	/**
	 * @param status the status to set
	 */
	public void setStatus(DiscountStatus status) {
		this.status = status;
	}

	/**
	 * @return the waybillNumber
	 */
	public String getWaybillNumber() {
		return waybillNumber;
	}

	/**
	 * @param waybillNumber the waybillNumber to set
	 */
	public void setWaybillNumber(String waybillNumber) {
		this.waybillNumber = waybillNumber;
	}

	/**
	 * @return the weight
	 */
	public Double getWeight() {
		return weight;
	}

	/**
	 * @param weight the weight to set
	 */
	public void setWeight(Double weight) {
		this.weight = weight;
	}

	/**
	 * @return the agentCode
	 */
	public String getAgentCode() {
		return agentCode;
	}

	/**
	 * @param agentCode the agentCode to set
	 */
	public void setAgentCode(String agentCode) {
		this.agentCode = agentCode;
	}

	/**
	 * @return the product
	 */
	public ProductMsg getProduct() {
		return product;
	}

	/**
	 * @param product the product to set
	 */
	public void setProduct(ProductMsg product) {
		this.product = product;
	}

	/**
	 * @return the rateType
	 */
	public RateTypeMsg getRateType() {
		return rateType;
	}

	/**
	 * @param rateType the rateType to set
	 */
	public void setRateType(RateTypeMsg rateType) {
		this.rateType = rateType;
	}

	/**
	 * @return the route
	 */
	public RouteMsg getRoute() {
		return route;
	}

	/**
	 * @param route the route to set
	 */
	public void setRoute(RouteMsg route) {
		this.route = route;
	}

	/**
	 * @return the fromNode
	 */
	public NodeMsg getFromNode() {
		return fromNode;
	}

	/**
	 * @param fromNode the fromNode to set
	 */
	public void setFromNode(NodeMsg fromNode) {
		this.fromNode = fromNode;
	}

	/**
	 * @return the toNode
	 */
	public NodeMsg getToNode() {
		return toNode;
	}

	/**
	 * @param toNode the toNode to set
	 */
	public void setToNode(NodeMsg toNode) {
		this.toNode = toNode;
	}

	/* (non-Javadoc)
	 * @see java.lang.Object#toString()
	 */
	@Override
	public String toString() {
		return "DiscountCriteria [code=" + code + ", action=" + action
				+ ", status=" + status + ", waybillNumber=" + waybillNumber
				+ ", weight=" + weight + ", agentCode="
				+ agentCode + ", product=" + product + ", rateType=" + rateType
				+ ", route=" + route + ", fromNode=" + fromNode + ", toNode="
				+ toNode + "]";
	}

}
