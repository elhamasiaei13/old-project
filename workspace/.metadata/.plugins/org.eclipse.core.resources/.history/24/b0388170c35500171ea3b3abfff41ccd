	 * @param fromNodeCode
	 */
	public void setFromNodeCode(String fromNodeCode) {
		this.fromNodeCode = fromNodeCode;
	}

	/**
	 * @return
	 */
	public String getToNodeCode() {
		return toNodeCode;
	}

	/**
	 * @param toNodeCode
	 */
	public void setToNodeCode(String toNodeCode) {
		this.toNodeCode = toNodeCode;
	}

	/**
	 * @return
	 */
	public String getCarrierCode() {
		return carrierCode;
	}

	/**
	 * @param carrierCode
	 */
	public void setCarrierCode(String carrierCode) {
		this.carrierCode = carrierCode;
	}

	/**
	 * @return
	 */
	public String getVehicleCarrierRegister() {
		return vehicleCarrierRegister;
	}

	/**
	 * @param vehicleCarrierRegister
	 */
	public void setVehicleCarrierRegister(String vehicleCarrierRegister) {
		this.vehicleCarrierRegister = vehicleCarrierRegister;
	}

	/**
	 * @return
	 */
	public TransportationScheduleMsg toMsg() {
		TransportationScheduleMsg msg = new TransportationScheduleMsg();
		
		msg.setId(this.getId());
		msg.setScheduled(this.scheduled);
		msg.setVehicleCarrierRegister(this.vehicleCarrierRegister);
		msg.setCarrierCode(this.carrierCode);
		msg.setRouteNumber(this.carrierRouteNumber);
		msg.setFromNodeCode(this.fromNodeCode);
		msg.setToNodeCode(this.toNodeCode);
		msg.setDepartureDateInOriginLocal(this.departureDateInOriginLocal);
		msg.setDepartureDateInUtc(this.departureDateInUtc);
		msg.setDepartureTimeInOriginLocal(this.departureTimeInOriginLocal);
		msg.setDepartureTimeInUtc(this.departureTimeInUtc);
		msg.setArrivalDateInUtc(this.arrivalDateInUtc);
		msg.setArrivalDateInDestinationLocal(this.arrivalDateInDestinationLocal);
		msg.setArrivalTimeInUtc(this.arrivalTimeInUtc);
		msg.setArrivalTimeInDestinationLocal(this.arrivalTimeInDestinationLocal);
		msg.setTotalWeight(this.totalWeight);
		msg.setReservableWeight(this.reservableWeight);
		msg.setStatus(TransportationManagementEnums.TransportationScheduleStatus.valueOf(this.status.toString()));
		
		return msg;
	}
	
	
	/* (non-Javadoc)
	 * @see java.lang.Object#toString()
	 */
	@Override
	public String toString() {
		return "TransportationScheduleVO [scheduled=" + scheduled + ", remarks=" + remarks + ", departureDateInUtc=" + departureDateInUtc
				+ ", departureDateInOriginLocal=" + departureDateInOriginLocal + ", departureTimeInUtc=" + departureTimeInUtc + ", departureTimeInOriginLocal="
				+ departureTimeInOriginLocal + ", departureInUtc=" + departureInUtc + ", arrivalDateInUtc=" + arrivalDateInUtc
				+ ", arrivalDateInDestinationLocal=" + arrivalDateInDestinationLocal + ", arrivalTimeInUtc=" + arrivalTimeInUtc
				+ ", arrivalTimeInDestinationLocal=" + arrivalTimeInDestinationLocal + ", arrivalInUtc=" + arrivalInUtc + ", totalWeight=" + totalWeight
				+ ", reservableWeight=" + reservableWeight + ", uldInfo=" + uldInfo + ", status=" + status + ", capacityDeterminant=" + capacityDeterminant
				+ ", carrierRouteNumber=" + carrierRouteNumber + ", vehicleId=" + vehicleId + "]";
	}

}
