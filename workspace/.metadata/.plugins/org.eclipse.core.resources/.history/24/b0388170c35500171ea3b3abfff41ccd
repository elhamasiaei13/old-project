	public void setReservableWeight(Double reservableWeight) {
		this.reservableWeight = reservableWeight;
	}

	/**
	 * @return the uldInfo
	 */
	public String getUldInfo() {
		return uldInfo;
	}

	/**
	 * @param uldInfo the uldInfo to set
